---
import TitleDetail from '../layouts/TitleDetail.astro';

interface Props {
    src: string,
};

const { src } = Astro.props;
---

<TitleDetail title='The prototype'>
    <iframe src={src} 
    class="aspect-video w-full" 
    allow="autoplay; fullscreen"
    allowfullscreen data-aos="fade-in" />
</TitleDetail>
